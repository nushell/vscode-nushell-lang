{
  "name": "vscode-nushell-lang",
  "displayName": "vscode-nushell-lang",
  "description": "nushell language for vscode",
  "version": "1.1.0",
  "preview": false,
  "license": "MIT",
  "publisher": "TheNuProjectContributors",
  "homepage": "https://www.nushell.sh/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nushell/vscode-nushell-lang.git"
  },
  "bugs": {
    "url": "https://github.com/nushell/vscode-nushell-lang/issues"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "icon": "assets/nushell.ico",
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onTerminalProfile:nushell_default"
  ],
  "main": "./dist/extension",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "package": "vsce package",
    "postpackage": "code --install-extension *.vsix"
  },
  "devDependencies": {
    "@types/node": "^12.20.47",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "vsce": "^2.9.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "keywords": [
    "nushell",
    "nu",
    "shell",
    "scripting"
  ],
  "galleryBanner": {
    "color": "#008000",
    "theme": "light"
  },
  "contributes": {
    "languages": [
      {
        "id": "nushell",
        "aliases": [
          "nushell",
          "nu"
        ],
        "extensions": [
          ".nu"
        ],
        "icon": {
          "light": "assets/nu.svg",
          "dark": "assets/nu.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nushell",
        "scopeName": "source.nushell",
        "path": "./syntaxes/nushell.tmLanguage.json"
      }
    ],
    "keybindings": [
      {
        "command": "start https://www.nushell.sh/book/",
        "key": "ctrl+f1",
        "when": "editorTextFocus && editorLangId == 'nushell'"
      },
      {
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus && editorLangId == 'nushell'",
        "mac": "cmd+alt+j",
        "win": "ctrl+alt+j",
        "linux": "ctrl+alt+j"
      }
    ],
    "snippets": [
      {
        "language": "nushell",
        "path": "./snippets/nushell.json"
      }
    ],
    "terminal": {
      "profiles": [
        {
          "title": "Nushell",
          "id": "nushell_default"
        }
      ]
    }
  },
  "dependencies": {
    "glob": "^7.2.0",
    "os": "^0.1.2"
  }
}
